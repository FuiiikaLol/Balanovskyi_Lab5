#include <iostream> 
using namespace std;

int main(){
    int matrica1[5][5]={
        {3,1,3,7,5},
        {3,2,3,4,5},
        {2,6,3,4,5},
        {1,4,3,4,5},
        {1,5,3,4,5}
        
    };
    cout<<"Уведіть величену"<<endl;
    int a;
   cin >> a;
    
    for(int i =0;i<5;i++){
       for(int j =0;j<5;j++){
            matrica1[i][j]+=a;
           cout<<matrica1[i][j]<<" ";
       }
      
       cout<<endl;
    }
 
 
}
